.package .packageItem p {
	font-size: 14px;
	color: #5C2C00;
	text-align: center;
	line-height: 14px;
	font-weight: bolder;
}


/*特权*/

.specialTitle {
	padding: 12px 12px 0 12px;
	border-radius: 8px;
	line-height: 14px;
	font-weight: bolder;
}

.specialTitle .mui-navigate-right:after,
.mui-push-right:after {
	padding-top: 13px;
}

.mui-navigate-right:after,
.mui-push-right:after {
	right: 12px;
}

.specialTitle a {
	font-size: 14px;
	color: #4A4A4A;
}

.specialXy {
	font-size: 12px;
	margin-right: 15px;
	float: right;
	color: #A4A4A4;
}

.specialTitle::before,
.specialTitle::after {
	background-color: transparent;
}

.specialContent {
	font-size: 12px;
	padding: 12px;
	color: #4A4A4A;
	text-align: justify;
	line-height: 18px;
	margin-bottom: 0;
}


/*激活码*/

.activeCode {
	margin-bottom: 15px;
}

.activeCodetext {
	border-radius: 8px;
	height: inherit;
	padding-left: 12px;
	font-weight: bolder;
}


.activeCodetext:active{
	background-color: #f2f2f2;
}
.activeCodetext::after,
.activeCodetext::before {
	background-color: transparent;
}

.activeCode a {
	font-size: 14px;
	color: #4A4A4A;
	line-height: 50px;
}

.footer {
	height: 60px;
}

.footerContent {
	height: inherit;
	width: 100%;
}

.footerR,
.footerL {
	height: 70px;
}

.footerL {
	background: #2A2A2A;
}

.footerL p {
	font-size: 12px;
	color: #fff;
	margin: 14px 0 -6px 12px;
}

span.num {
	color: #FFC48D;
}

.footerL p b {
	font-size: 20px;
	padding: 0 3px;
}

.footerL p span:nth-child(2) {
	padding-left: 12px;
}

.footerL .footerCheck {
	font-size: 12px;
	color: #fff;
	line-height: 12px;
	margin: 0 0 16px 12px;
}

.footerL .footerCheck span {
	color: #fbc18b;
	font-weight: bolder;
	position: relative;
	bottom: 23px;
	left: 92px;
}

.footerR {
	text-align: center;
}

.mui-btn.mui-disabled.payBtn,
.mui-btn.mui-disabled.activeBtn,
.mui-btn.payBtn:disabled,
.mui-btn.activeBtn:disabled,
button.payBtn.mui-disabled,
button.activeBtn.mui-disabled,
button.payBtn:disabled,
button.activeBtn:disabled {
	background-image: linear-gradient(134deg, #F7D891 0%, #CD7B36 100%);
	color: #5C2C00;
}

