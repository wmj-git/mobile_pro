	padding-bottom: 20px;
}
.packageItem embed{
	width: 36px;
	height: 36px;
}
.package .packageItem{
	padding: 0 0 30px;
}
.package.payPackage .packageItem:nth-child(1),.package.payPackage .packageItem:nth-child(2),.package.payPackage .packageItem:nth-child(3){
	padding-top: 30px !important;
}

