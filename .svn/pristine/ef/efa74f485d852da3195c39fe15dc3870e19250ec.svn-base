html body{
	/*font-family: "Heiti SC,Helvetica,HelveticaNeue,Droidsansfallback,Droid Sans";*/
	font-family: -apple-system, BlinkMacSystemFont, "PingFang SC","Helvetica Neue",STHeiti,"Microsoft Yahei",Tahoma,Simsun,sans-serif;
}
body {
	background-color: #f2f2f2;	
}

body.index,
body.active {
	background-color: #2A2A2A;
}

body.finsh {
	background-color: #fff;
}

.mui-action-back::before {
	content: "";
	background: url("../images/nav_return.svg") no-repeat center;
	width: 22px;
	height: 22px;
	display: inline-block;
}

.mui-navigate-right:after,
.mui-push-right:after {
	font-size: 16px;
}

.overfolwText {
	text-overflow: ellipsis;
	overflow: hidden;
}

.mui-bar {
	box-shadow: unset;
}

.bg {
	background: #2A2A2A;
	padding-bottom: 100px;
	position: relative;
	top: 0;
	border-radius: 0;
}

.headerFixed {
	background: #2A2A2A;
	height: 70px;
	display: flex;
	justify-content: center;
	align-items: center;
	box-shadow: unset;
	padding:8px 12px 0px
}

.headerFixed.aggrementHeader {
	height: auto;
}

.aggrementTitle {
	text-align: center;
	color: #dca31f;
	font-size: 16px;
	font-weight: bold;
}

.title {
	text-align: center;
	font-size: 14px;
}

.service {
	text-align: right;
}
.embed.iconService{
	padding-top:2px;
}
.user {
	margin: 0 12px;
	border-radius: 8px;
	position: relative;
	top: -66px;
}

.userRow {
	height: 86px;
	margin-top: 26px;
	background: url(../images/acount_bg_gray@3x.png) no-repeat center right;
	background-size: contain;
	background-color: #fff;
	border-radius: 8px;
	box-shadow: 0 4px 12px 0 rgba(0, 0, 0, 0.08);
}

.userRow.membersUser {
	background: url(../images/acount_bg_colorful@3x.png) no-repeat center right;
	background-size: contain;
	background-color: #fff;
	border-radius: 8px;
	padding: 0 12px 0 0;
	height: 86px;
	margin-top: 26px;
	box-shadow: 0 4px 12px 0 rgba(0, 0, 0, 0.08);
}

.headerImg {
	padding-left: 12px;
	position: relative;
}

.img_bg,
.img_header {
	position: absolute;
}

.img_bg {
	top: 22px;
	box-shadow: 0px 4px 10px rgba(0, 0, 0, .1);
	border-radius: 50%;
	z-index: 1;
}

.img_header {
	top: 11.5px;
	left: 39px;
}

.userInfo {
	position: relative;
	text-align: left;
	left: 12px;
	top: 24px;
}

.countInfo {
	display: inline;
	color: #4A4A4A;
	font-size: 16px;
	margin-bottom: 0;
}
.countInfo span.loginInfo{
	display: inline-block;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    line-height: 16px;
    max-width: 112px;
}
.countInfo span:nth-child(2) {
	font-size: 12px;
	margin: 0 8px;
}

.changeCount {
	text-decoration: underline;
	color: #62340a;
	font-weight: bolder;
	/*position: absolute;
    right: 20px;*/
}

.changeCount.loginBtn {
	display: inline-block;
	width: 80px;
	height: 26px;
	position:absolute;
	margin-right: 12px;
	background-image: linear-gradient(134deg, #F7D891 0%, #CD7B36 100%);
	border-radius: 15.5px;
	text-decoration: none;
	border-radius: 15.5px;
	font-size: 12px;
	color: #5C2C00;
	letter-spacing: 0;
	text-align: center;
	font-weight: bolder;
	right: 12px;
	line-height: 26px;
	margin-top: 6px;
}

.userTitle {
	font-size: 12px;
	color: #000000;
	line-height: 18px;
}

.userContext {
	font-size: 12px;
	color: rgba(74, 74, 74, 0.5);
	letter-spacing: 0;
}

.members,
.pay,
.package,
.activeCode,
.special,
.activeInput,
.activeIntroduce {
	background: #FFFFFF;
	box-shadow: 0 4px 12px 0 rgba(0, 0, 0, 0.08);
	border-radius: 8px;
}

.activeIntroduce {
	border-radius: 8px 8px 0 0;
	box-shadow: unset;
}

.userRow,
.members,
.pay,
.package,
.special,
.activeCode {
	margin-bottom: 12px;
}


/*套餐*/

.membersType {
	display: none;
}

.membersTitle,.specialBox {
	font-size: 14px;
	color: #4A4A4A;
	line-height: 14px;
	padding: 16px 0 16px 12px;
	font-weight: bolder;
}

.mebersCheckBox {
	padding: 0 12px;
	position: relative;
}

.membersContent {
	text-align: center;
	padding: 0 12px;
	display: flex;
	justify-content: center;
	align-items: center;
}

.membersItem {
	border: .5px solid rgba(0, 0, 0, .3);
	border-radius: 4px;
	height: 100px;
	line-height: 22px;
	font-size: 12px;
	color: #000;
	background: url(../images/checkbox_price_selected_gray.svg) no-repeat bottom right;
}

.membersItem.membersItem_active {
	background: url(../images/checkbox_price_selected.svg), url(../images/img_bgvr.svg);
	background-repeat: no-repeat;
	background-position: bottom right;
	background-color: rgba(0, 0, 0, .8);
	border: none;
}

.membersItem.membersItem_active .listTitle,
.membersItem.membersItem_active .newPrice,
.membersItem.membersItem_active .oldPrice,
.membersItem.membersItem_active .lastPrice {
	color: #FBCA9C;
}

.listTitle {
	padding-top: 6px;
}

.listTitle,
.newPrice,
.oldPrice,
.lastPrice {
	margin-bottom: 0;
	font-size: 12px;
	color: #4A4A4A;
}
.newPrice,.newPrice .new{
	font-weight: bolder;
}
.newPrice .new {
	font-size: 18px;
}

.oldPrice {
	text-decoration: line-through;
}

.membersItem:nth-child(2) {
	margin: 0 12px;
}

.mebersCheck {
	font-size: 14px;
	color: #A4A4A4;
	line-height: 14px;
	vertical-align: middle;
}

.membersTooltip {
	display: none;
	background: url(../images/popover_ad.svg) no-repeat center;
	position: absolute;
	top: 26px;
	left: 4px;
	width: 110px;
	height: 31px;
	line-height: 36px;
	background-size: cover;
	z-index: 1;
}

.membersTooltip p {
	font-weight: bolder;
	font-size: 12px;
	margin-bottom: 0;
	color: #5C2C00;
	letter-spacing: 0;
	text-align: center;
	transform: scale(0.9);
}


/*取消连包会员混点*/

.movies,
.moviesTicket {
	background-color: #fff;
	padding: 16px 12px;
	border-radius: 4px;
	display: none;
}

.movies ul,
.moviesTicket ul {
	padding-left: 0;
	margin: 0;
}

li.moviesItem {
	list-style: none;
	border: .5px solid #979797;
	background: url(../images/checkbox_price_selected_gray.svg) no-repeat right bottom;
	border-radius: 4px;
	padding: 0;
	font-size: 12px;
	text-overflow: ellipsis;
	white-space: nowrap;
}

li.moviesItem:nth-child(3) {
	height: 70px;
	display: grid;
	align-items: center;
}

li.moviesItem.moviesItem_active:nth-child(3) {
	height: 69px;
	display: grid;
	align-items: center;
}

li.moviesItem:nth-child(2) {
	margin: 16px 0;
}

li.moviesItem.moviesItem_active {
	background: url(../images/checkbox_price_selected.svg), url(../images/img_bgvr.svg);
	background-repeat: no-repeat;
	background-position: bottom right;
	background-color: rgba(0, 0, 0, .8);
	border: none;
	color: #FBCA9C;
}

li.moviesItem.moviesItem_active .moviesR,
.moviesItem_active .moviesTicketL p,
.moviesItem_active .moviesR P,
.moviesItem_active .moviesR span,
.moviesItem_active .moviesR span.new {
	color: #FBCA9C;
}

.moviesMembers {
	padding: 0 0 12px 12px;
}

.moviesMembers.oldPriceShop {
	padding: 0 0 0 12px;
}

.moviesTitle {
	font-size: 14px;
	color: #4A4A4A;
	line-height: 14px;
	margin-bottom: 8px;
	font-weight: bolder;
}

.moviesTicketL p.moviesTime {
	margin: auto;
}

.moviesTime {
	font-size: 12px;
	color: #4A4A4A;
	line-height: 12px;
	margin-bottom: 16px;
}

.moviesItem .moviesItemer {
	font-weight: bolder;
	font-size: 14px;
}

.movies .moviesL,
.moviesTicket .moviesL {
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
	padding: 12px 0 0 12px;
	;
	font-size: 14px;
}

.moviesL::after,
.moviesL1::after {
	content: "+";
	position: relative;
	left: 30%;
	font-weight: bolder;
}

.moviesL1::after {
	left: 25%;
}

.moviesTicket p.ticketOp,
.moviesTicket p.ticketP {
	margin: auto;
	font-size: 12px;
}

.moviesTicket p.ticketP {
	padding-top: 12px;
}

.moviesTicket .moviesR,
.movies .moviesR {
	font-size: 12px;
	color: #4A4A4A;
	padding-right: 12px;
	line-height: 12px;
	text-align: right;
	font-weight: bold;
}

.moviesItemer .moviesCenter,
.moviesItemer .moviesRight {
	padding-top: 12px;
}

.moviesR.moviesCanacel {
	padding-top: 4px;
}

.moviesR.flexLayout {
	justify-content: flex-end;
}

.moviesTicket .moviesTicketL {
	line-height: 66px;
	padding-left: 12px;
}

.throuth {
	text-decoration: line-through;
	line-height: 20px;
}

.moviesR .ticketOp span {
	font-size: 12px;
}

.moviesR span.new {
	font-size: 18px;
	line-height: 24px;
	color: #4A4A4A;
}


/*支付方式*/

.pay::before,
.pay::after {
	background-color: transparent;
}

.pay .text {
	font-size: 14px;
	color: #4A4A4A !important;
	font-weight: bold;
}

/*.pay .mui-icon {
	margin-right: 12px;
	vertical-align: bottom;
}

.pay .mui-icon.icon-zfb {
	color: #02A9F1;
	font-size: 24px;
}

.pay .mui-icon.icon-my_weixin {
	color: #09BB07;
	font-size: 24px;
}*/
.pay .icon-pay{
	font-size: 24px;
	margin-right: 12px;
	vertical-align: bottom;
}

.mui-table-view-radio .mui-table-view-cell {
	padding-left: 12px;
}

.mui-table-view-cell:after {
	left: 12px;
	right: 12px;
	height: 0;
}

.mui-table-view-radio .mui-table-view-cell .mui-navigate-right:after {
	background: url(../images/circlecheckbox_l_default.svg) no-repeat center;
	color: transparent;
	width: 30px;
	height: 30px;
}

.mui-table-view-radio .mui-table-view-cell.mui-selected .mui-navigate-right:after {
	background: url(../images/checkbox_l_elected.svg) no-repeat center;
	color: transparent;
}


