.footerR .payBtn {
	font-size: 18px;
	color: #5C2C00;
	height: inherit;
	border: none;
	top: 0;
	padding: 0;
	width: 100%;
	background-image: linear-gradient(134deg, #F7D891 0%, #CD7B36 100%);
	font-weight: bolder;
	border-radius: 0;
}

.mui-checkbox.mui-left input[type=checkbox],
.mui-radio.mui-left input[type=radio] {
	left: -2px;
}

.mui-checkbox input[type=checkbox],
.mui-radio input[type=radio] {
	top: 6px;
}

.mui-checkbox.mui-left label,
.mui-radio.mui-left label {
	padding-left: 20px;
}

.mui-checkbox input[type=checkbox]:checked:before,
.mui-radio input[type=radio]:checked:before {
	color: transparent;
	background: url(../images/checkbox_rectangle_selected.svg) no-repeat center;
}

.mui-checkbox input[type=checkbox]:before,
.mui-radio input[type=radio]:before {
	color: transparent;
	background: url(../images/checkbox_s_selected.svg)no-repeat center;
	vertical-align: middle;
	font-size: unset;
}


/*激活码页面激活码按钮*/

.footerActive {
	height: 60px;
	box-shadow: 0 5px 11px 0 rgba(0, 0, 0, 0.05);
}

.mui-bar .mui-btn.activeBtn {
	top: 0;
	border-radius: 0;
}

.activeBtn {
	font-size: 18px;
	color: #5C2C00;
	height: inherit;
	border: none;
	background-image: linear-gradient(134deg, #F7D891 0%, #C05C18 100%);
	padding: 0;
	width: 100%;
	font-weight: bolder;
	border-radius: 0;
}

.activeInput {
	height: 76px;
}

.line {
	height: .5px;
	background-color: rgba(0, 0, 0, 0.1);
	margin: 0 12px;
}

.activeInput input[type=color],
input[type=date],
input[type=datetime-local],
input[type=datetime],
input[type=email],
input[type=month],
input[type=number],
input[type=password],
input[type=search],
input[type=tel],
input[type=text],
input[type=time],
input[type=url],
input[type=week],
select,
textarea {
	border: none;
	background: rgba(0, 0, 0, 0.05);
	border-radius: 4px;
	font-size: 14px;
	color: #4A4A4A;
	text-align: justify;
	line-height: 16px;
	height: 44px;
	margin: 16px 12px;
	width: -webkit-fill-available;
	padding-top: 15px;
}


/*激活说明*/

.activeIntroduce {
	margin-top: 12px;
	height: 100%;
	min-height: 700px;
	overflow: hidden;
}

.activeTitle {
	font-weight: bolder;
	font-size: 14px;
	color: #4A4A4A;
	padding: 12PX 0 0 12PX;
	margin: 0;
}

.activeText {
	font-size: 12px;
	color: #4A4A4A;
	text-align: justify;
    padding: 2px 12px 0;
	margin: 0;
}
.activeText:nth-child(2) {
	padding-top: 6px;
}
.payPage {
	background-color: #fff;
}


/*支付页面*/

.payPage .header,
.payPage .payfooter {
	height: 60px;
	display: flex;
	justify-content: center;
	align-items: center;
}

.payPage .header {
	background: #2A2A2A;
}

.payPage .header .back a {
	display: inline-block;
	width: inherit;
	height: inherit;
	padding-top: 14px;
}

.payPage.header .title {
	text-align: center;
}

.payPage .header .service {
	text-align: right;
}

.scrollHead {
	background-color: #F2F2F2;
}

.scrollHead.soleHead {
	position: absolute;
	left: 0;
	bottom: 0;
	top: 110px;
	right: 0;
}

embed.iconWaiting {
	margin-bottom: 4px;
	animation: glow 2s ease-in-out infinite;
}


/*支付页面中部*/

.dealWith {
	text-align: center;
	display: flex;
	align-items: center;
	justify-content: center;
	background: #fff;
}


/*处理中*/

.dealWith.payWaiting {
	margin-top: 0;
	padding: 0 12px;
	position: absolute;
	left: 0;
	top: 0;
	right: 0;
	bottom: 0;
}

.dealWith .package {
	display: none;
	margin-bottom: 0;
	border-radius: 0;
	box-shadow: unset;
}


/*已完成*/

.dealWith.dealing {
	padding-top: 90px;
}
.dealWith.dealing.soleWaiting{
	padding: 180px 12px 0;
}
.finsh_line {
	height: .5px;
	background:rgba(0,0,0,.1);
	display: block;
	margin-top: 30px;
}

.dealWith .payWaiting {
	margin: 50% auto;
}

.dealWith.payWaiting p.manage,
.dealWith.payFinshed p.manage {
	font-size: 20px;
	color: #4A4A4A;
	font-weight: bolder;
}

.dealWith .payFinshed p.success {
	font-size: 16px;
	color: #4A4A4A;
	font-weight: bold;
}

.dealWith .waitingText {
	margin-bottom: 10%;
}

.dealWith .waitingText p {
	font-size: 14px;
	color: #4A4A4A;
	margin-bottom: 5px;
}

.dealWith .waitingText p span {
	color: #F78C16;
}
.Ticket {
    padding: 0 12px;
}
.Ticket,
.membersAndTicket,
.membersSuccess {
	display: none;
}


/*支付页面底部*/

.payfooter .footerL,
.payfooter .footerR {
	height: 60px;
}

.payfooter .footerL {
	padding: 0;
}

.footerL .repeatBtn,
.footerR .finshBtn {
	font-size: 18px;
	height: inherit;
	border: none;
	top: 0;
	padding: 0;
	width: 100%;
	font-weight: bolder;
	border-radius: 0;
}

.footerL.repeatFooter {
	background-color: #2A2A2A;
}

.footerR .finshBtn {
	color: #5C2C00;
	background-image: linear-gradient(134deg, #F7D891 0%, #CD7B36 100%);
}

.footerL .repeatBtn {
	color: #fff;
	background-color: rgba(28, 28, 28, 0.9);
}

.footerL.repeatFooter::focus-wthin {
	background-color: #929292;
}


/*协议内容*/

.xyContent {
	background-color: #fff;
	padding: 12px;
	margin-top: 50px;
}

.xyContent h4 {
	text-align: center;
}

.xy_company {
